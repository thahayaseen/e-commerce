        </nav>
    </header>

    <div class="container mt-4">
        <div class="row">
            <div class="col-lg-3 my-lg-0 ">
                <%-include('./sidebar',{activePage:"orders"}) %>

            </div>
            <div class="col-lg-9 my-lg-0">

                <% orders.forEach(order=> { %>
                    <div class="order-card">
                        <div class="order-header">
                            <span class="order-id">Order #<%= order.orderid %></span>
                            <div class="d-flex gap-3">
                                <span
                                    class="status-badge <%= order.status === 'Delivered' ? 'status-delivered' : 'status-processing' %>">
                                    <%= order.status %>
                                </span>
                                <button data-orderid="<%=order._id%>"
                                    id="<%=order.paymentStatus=='Failed'? 'retrypayment':'order'  %>"
                                    class="status-badge <%= order.paymentStatus === 'Paid' ? 'status-delivered' : 'status-processing' %>">
                                    Payment:
                                    <%if(order.status=='Cancelled'&&order.paymentStatus==='Paid'){%>
                                        <%= 'Refunded' %>
                                            <%}else if((order.paymentStatus=='Paid'||order.paymentStatus=='Pending')&&order.status!=='Cancelled'){%>
                                                <%= order.paymentStatus %>
                                           <%} else if (order.paymentStatus==='Failed' ) { %>
                                                <%= order.paymentStatus + ' - Retry' %>
                                                    <% } else { %>
                                                       None
                                                            <% } %>
                                </button>
                                <button class="btn btn-details"
                                    onclick="viewOrderDetails('<%= JSON.stringify(order) %>')">
                                    <i class="fas fa-info-circle me-1"></i> Details
                                </button>
                            </div>
                        </div>

                        <ul class="product-list">
                            <% order.products.forEach(product=> { %>
                                <li class="product-item">
                                    <div class="product-info">
                                        <img src="/uploads/<%= product.productid.images[0] %>"
                                            alt="<%= product.productid.name %>" class="product-image"
                                            onerror="this.onerror=null; this.src='/uploads/default-image.png';">
                                        <div class="product-details">
                                            <h5>
                                                <%= product.productid.name %>
                                            </h5>
                                            <span class="product-price">₹<%= product.price %> x <%= product.quantity %>
                                                        </span>
                                        </div>
                                    </div>
                                    <div class="action-buttons">
                                        <button class="btn btn-view"
                                            onclick="viewProduct('<%=JSON.stringify(product.productid)%>')">
                                            <i class="fas fa-eye me-1"></i> View
                                        </button>
                                        <% if (order.status !=='Delivered' &&order.status !=='Cancelled'
                                            &&product.status==true) { %>
                                            <button class="btn btn-danger cancelbtn" data-productid="<%=product._id%>"
                                                data-orderid="<%=order.orderid  %>">
                                                <i class="fas fa-undo-alt me-1"></i> Cancel
                                            </button>
                                            <% } else if (product.status==false) { %>
                                                <button class="btn btn-primary">
                                                    <i class="fas fa-check"></i> Cancelled
                                                </button>
                                                <% } else if (product.return=='notrequst' ) { %>
                                                    <button class="btn btn-return"
                                                        onclick="initiateReturn('<%= order._id %>', '<%= product.productid._id %>')">
                                                        <i class="fas fa-undo-alt me-1"></i> Return
                                                    </button>
                                                    <% } else if(product.status !=='Cancelled' ){ %>
                                                        <button class="btn btn-return">
                                                            <i class="fas fa-undo-alt me-1"></i>
                                                            <%= product.return %>
                                                        </button>
                                                        <% } %>
                                    </div>
                                </li>
                                <% }) %>
                        </ul>


                        <div class="p-3 d-flex justify-content-between align-items-center">
                            <div>
                                <strong>Order Date:</strong>
                                <%= new Date(order.createdAt).toLocaleDateString() %>
                            </div>
                            <div>
                                <strong>Total:</strong> ₹<%= (order.totalAmount-order.refund - (order.coupon ?
                                    order.coupon.discount : 0)+order.shippingcharg) %>
                            </div>
                        </div>
                    </div>
                    <% }) %>

                        <!-- Pagination -->
                        <nav aria-label="Order pagination">
                            <ul class="pagination justify-content-center">
                                <% if (currentPage> 1) { %>
                                    <li class="page-item">
                                        <a class="page-link" href="?page=<%= currentPage - 1 %>">Previous</a>
                                    </li>
                                    <% } %>

                                        <% for (let i=1; i <=totalPages; i++) { %>
                                            <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                                <a class="page-link" href="?page=<%= i %>">
                                                    <%= i %>
                                                </a>
                                            </li>
                                            <% } %>

                                                <% if (currentPage < totalPages) { %>
                                                    <li class="page-item">
                                                        <a class="page-link"
                                                            href="?page=<%= Number(currentPage) + 1 %>">Next</a>
                                                    </li>
                                                    <% } %>
                            </ul>
                        </nav>
            </div>
        </div>
    </div>

    <!-- Product Modal -->
    <div class="modal fade" id="productModal" tabindex="-1">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Product Details</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <img id="modalProductImage" src="" alt="Product" class="img-fluid rounded">
                        </div>
                        <div class="col-md-6">
                            <h4 id="modalProductName"></h4>
                            <p id="modalProductDescription" class="text-muted"></p>
                            <h5 id="modalProductPrice" class="text-primary"></h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="returnModal" tabindex="-1">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Return Product</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body">
                    <form id="returnForm">
                        <div class="mb-3">
                            <label class="form-label">Return Reason</label>
                            <select class="form-select" id="returnReason" required>
                                <option value="">Select a reason</option>
                                <option value="damaged">Damaged/Defective</option>
                                <option value="not_as_described">Not as Described</option>
                                <option value="changed_mind">Changed Mind</option>
                                <option value="other">Other</option>
                            </select>
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Additional Details</label>
                            <textarea class="form-control" id="returnDetails" rows="3" required></textarea>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" onclick="submitReturn()">Submit Return</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="orderDetailsModal" tabindex="-1">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Order Details</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body">
                    <div class="order-summary mb-4">
                        <h6 class="mb-3">Order Summary</h6>
                        <div class="order-summary-item">
                            <span>Order ID:</span>
                            <span id="modalOrderId"></span>
                        </div>
                        <div class="order-summary-item">
                            <span>Order Date:</span>
                            <span id="modalOrderDate"></span>
                        </div>
                        <div class="order-summary-item">
                            <span>Status:</span>
                            <span id="modalOrderStatus"></span>
                        </div>
                        <div class="order-summary-item">
                            <span>Payment Status:</span>
                            <span id="modalPaymentStatus"></span>
                        </div>
                    </div>

                    <div class="product-details mb-4">
                        <h6 class="mb-3">Products</h6>
                        <div id="modalProductsList"></div>
                    </div>

                    <div class="shipping-details mb-4">
                        <h6 class="mb-3">Shipping Details</h6>
                        <div id="modalShippingDetails"></div>
                    </div>

                    <div class="price-summary">
                        <h6 class="mb-3">Price Details</h6>

                        <div class="order-summary-item">
                            <span>Subtotal:</span>
                            <span id="modalSubtotal"></span>
                        </div>
                        <div class="order-summary-item">
                            <span>Shipping:</span>
                            <span id="modalShipping"></span>
                        </div>
                        <div class="order-summary-item" id="modalDiscountContainer">
                            <span>Coupon Discount:</span>
                            <span id="modalDiscount"></span>
                        </div>
                        <div class="order-summary-item">
                            <strong>Total:</strong>
                            <strong id="modalTotal"></strong>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <a type="button" id="invoicebtn" class="btn btn-invoice disabled" onclick="downloadInvoice()">
                        <i class="fas fa-download me-1"></i> Download Invoice
                    </a>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/user/orders.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        let currentOrderId = null;
        let currentProductId = null;

        function viewProduct(productData) {
            console.log(productData);
            const product = JSON.parse(productData)

            document.getElementById('modalProductImage').src = `/uploads/${product.images[0]}`;
            document.getElementById('modalProductName').textContent = product.name;
            document.getElementById('modalProductDescription').textContent = product.description;
            document.getElementById('modalProductPrice').textContent = `₹${product.price.toFixed(2)}`;

            new bootstrap.Modal(document.getElementById('productModal')).show();
        }

        function initiateReturn(orderId, productId) {
            console.log('hallo');
            
            currentOrderId = orderId;
            currentProductId = productId;
            new bootstrap.Modal(document.getElementById('returnModal')).show();
        }

        function submitReturn() {
            const returnReason = document.getElementById('returnReason').value;
            const returnDetails = document.getElementById('returnDetails').value;

            if (!returnReason || !returnDetails) {
                alert('Please fill all the required fields');
                return;
            }

            const data = {
                returnReason: returnReason,
                returnDetails: returnDetails,
                orderid: currentOrderId
            };

            fetch(`/return/${currentProductId}`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify(data)
            })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        alert('Return request submitted successfully');
                        location.reload();
                    } else {
                        alert('Error submitting return request');
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    alert('Error submitting return request');
                });

            bootstrap.Modal.getInstance(document.getElementById('returnModal')).hide();
        }
        function viewOrderDetails(orderData) {
            const order = JSON.parse(orderData);

            // Set basic order information
            document.getElementById('modalOrderId').textContent = order.orderid;
            document.getElementById('modalOrderDate').textContent = new Date(order.createdAt).toLocaleDateString();
            document.getElementById('modalOrderStatus').textContent = order.status;
            document.getElementById('modalPaymentStatus').textContent = order.paymentStatus;
            
            //  products list
            const productsList = document.getElementById('modalProductsList');
            productsList.innerHTML = order.products.map(product => {
                if (product.status) {
                    return `<div class="product-item">
                    <div class="product-info">
                        <img src="/uploads/${product.productid.images[0]}" alt="${product.productid.name}" class="product-image">
                        <div class="product-details">
                            <h5>${product.productid.name}</h5>
                            <del class="product-price">${product.price}</del>
                            <span class="product-price">₹${product.price - product.discount} x ${product.quantity}</span>
                        </div>
                    </div>
                </div>
            `
                } else {
                    return `<div class="product-item">
                    <div class="product-info">
                        <img src="/uploads/${product.productid.images[0]}" alt="${product.productid.name}" class="product-image">
                        <div class="product-details">
                        <h5>${product.productid.name}</h5>
                        <del class="product-price">${product.price}</del>
                        <span class="product-price">₹${product.price - product.discount} x ${product.quantity}</span>
                        </div>
                        </div>
                        <button class="btn">Canceled</button>
                </div>
            `
                }
            }).join('');

            // Set shipping details
            document.getElementById('modalShippingDetails').innerHTML = `
                <p class="mb-1">${order.shippingAddress.fullname || ''}</p>
                <p class="mb-1">${order.shippingAddress.addressline1 + ' ' + order.shippingAddress.addressline2 || ''}</p>
                <p class="mb-1">${order.shippingAddress.city || ''}, ${order.shippingAddress.state || ''} ${order.shippingAddress.zipcode || ''}</p>
                <p class="mb-1">Phone: ${order.shippingAddress.country}</p>
                <p class="mb-1">Phone: ${order.shippingAddress.phone}</p>
            `;

            // Set price details

            let canceledproductprice = 0
            let coupondiscount = 0
            let persentage = 0
            let shipping = 0
            order.products.map(product => {
                console.log(product.status);
                if (!product.status) {
                    let productprice = product.price - product.discount

                    if (order.coupon.couponcode) {
                        persentage = (order.coupon.discount * 100) / order.totalAmount
                    }
                    coupondiscount += Math.round((((productprice * persentage) / 100) * product.quantity))


                    canceledproductprice += Math.floor(productprice * product.quantity)


                }

            })
            console.log('toatal product price' + canceledproductprice);
            console.log('toatal coupon discount' + coupondiscount);
            console.log('toatal coupon discount' + order.coupon.discount);
            console.log('toatal coupon total' + order.totalAmount);
            console.log('is', order.refund);


            console.log('toatal coupon price' + order.coupon.discount - coupondiscount || 0);

            console.log('discout price' + coupondiscount);
            let toatals = Math.floor((order.totalAmount - canceledproductprice || 0) - (order.coupon.discount - coupondiscount || 0) + shipping);

            console.log(toatals);

            const subtotal = order.totalAmount - canceledproductprice;
            document.getElementById('modalSubtotal').textContent = `₹${subtotal.toFixed(2)}`;
